source "https://rubygems.org"

# 主题 gem
gem "jekyll-theme-minimal-mistakes-jekyll"

# Jekyll 的核心 gem
gem "jekyll", "~> 4.2"
gem "jekyll-include-cache", "~> 0.2.1"

# 你之前使用的 Jekyll 插件
gem "jekyll-sitemap"
gem "jekyll-feed"
gem "jekyll-paginate"
gem "jekyll-seo-tag"

# 在本地开发时，你可以使用这个 gem 来预览你的网站
group :jekyll_plugins do
  gem "jekyll-sitemap"
  gem "jekyll-feed"
end
